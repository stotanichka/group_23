		{
			"name": "lesson6.2.user_info",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"var jsonData = pm.response.json();",
							"",
							"const schema = {",
							"    \"type\": \"object\",",
							"    \"properties\": {",
							"        \"person\": {\"type\" : \"object\"}, ",
							"        \"qa_salary_after_12_months\": {\"type\": \"number\"}, ",
							"        \"qa_salary_after_6_months\": {\"type\": \"integer\"}, ",
							"        \"start_qa_salary\": {\"type\": \"integer\"}, ",
							"    },",
							"    \"required\": [",
							"        \"person\",",
							"        \"qa_salary_after_12_months\",",
							"        \"qa_salary_after_6_months\",",
							"        \"start_qa_salary\"",
							"    ]",
							"}",
							"pm.test(\"Schema\", function () {",
							"    pm.response.to.have.jsonSchema(schema);",
							"});",
							"",
							"// pm.test(\"Structure\", function () {",
							"//     var data = [\"person\", \"qa_salary_after_12_months\", \"qa_salary_after_6_months\", \"start_qa_salary\"];",
							"//     var inetrnal_data = [\"u_age\", \"u_name\", \"u_salary_1_5_year\"];",
							"//     pm.expect(JSON.stringify(Object.keys(jsonData))).to.eql(JSON.stringify(data));",
							"//     pm.expect(JSON.stringify(Object.keys(jsonData.person))).to.eql(JSON.stringify(inetrnal_data));",
							"// });",
							"var parsedRequest = JSON.parse(pm.request.body.raw);",
							"pm.test(\"u_salary_1_5_year\", function () {",
							"    pm.expect(jsonData.person['u_salary_1_5_year']).to.eql(4*(parsedRequest.salary));",
							"});",
							"pm.test(\"qa_salary_after_6_months\", function () {",
							"    pm.expect(jsonData['qa_salary_after_6_months']).to.eql(2*(parsedRequest.salary));",
							"});",
							"pm.test(\"qa_salary_after_12_months\", function () {",
							"    pm.expect(jsonData['qa_salary_after_12_months']).to.eql(2.9*(parsedRequest.salary));",
							"});",
							"pm.test(\"start_qa_salary\", function () {",
							"    pm.expect(jsonData[\"start_qa_salary\"]).to.eql(+parsedRequest.salary);",
							"});",
							"pm.test(\"u_age\", function () {",
							"    pm.expect(jsonData.person['u_age']).to.eql(+parsedRequest.age);",
							"});",
							"pm.test(\"u_name\", function () {",
							"    pm.expect(JSON.stringify(jsonData.person['u_name'])).to.eql(JSON.stringify([parsedRequest.name, +parsedRequest.salary, +parsedRequest.age]));",
							"});",
							"pm.environment.set(\"salary\",jsonData.person['u_salary_1_5_year']);"
						],
						"type": "text/javascript"
					}
				}
			],
